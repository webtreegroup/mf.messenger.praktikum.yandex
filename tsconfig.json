{
  "compileOnSave": true,
  "compilerOptions": {
    "module": "commonjs",
    "target": "esnext",
    "rootDir": "./",
    "outDir": "./static",
    "esModuleInterop": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "watch": true,
    "lib": [
      "es2019",
      "dom",
      "dom.iterable"
    ],
    "downlevelIteration": true,
  }
}
