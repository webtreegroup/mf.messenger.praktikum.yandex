{
  "compilerOptions": {
    "rootDir": "./",
    "target": "es6",
    "strict": true,
    "declaration": false,
    "strictNullChecks": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "alwaysStrict": true,
    "lib": [
      "dom",
      "es6",
      "dom.iterable",
      "es2017"
    ],
    "outDir": "./static",
    "plugins": [
      {
        "transform": "@zoltu/typescript-transformer-append-js-extension/output/index.js",
        "after": true
      }
    ]
  }
}
